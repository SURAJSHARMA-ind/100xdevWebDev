generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  password String
}

model Todos {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  status      Boolean @default(false)
  user_id     Int
}
